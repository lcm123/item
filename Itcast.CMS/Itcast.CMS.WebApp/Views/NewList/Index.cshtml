            <div class="cont">
            	<ul class="list_news">
                    @if(ViewData["newInfoList"]!=null)
                    {
                        foreach(T_New newInfo in (List<T_New>)ViewData["newInfoList"])
                        {
            	  <li><span>@newInfo.SubDateTime.ToShortDateString()</span><a href="/NewList/ShowDetail/?id=@newInfo.Id" target="_blank">@newInfo.Title</a></li>
                       }
                 }
                    else
                    {
                        <li>暂无数据</li>
                    }
            	</ul>
                <div class="page_nav">@MvcHtmlString.Create(PageBar.GetPageBar((int)ViewData["pageIndex"],(int)ViewData["pageCount"]))</div>
            </div>
        </div>
      	<div class="clear"></div>
     </div>
<div id="footer_column">
